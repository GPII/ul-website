    "url": "https://issues.gpii.net/browse/UL"
  },
  "homepage": "https://github.com/GPII/ul-website",
  "dependencies": {
    "foundation-sites": "^6.3.0",
    "gpii-binder": "^1.0.0-dev.20170127T144054Z.54a5ef2",
